{
  "name": "autosuggest",
  "version": "1.0.9",
  "description": "Simple autocomplete component in vanilla JS",
  "author": "Grzegorz Tomicki",
  "main": "docs/autosuggest.min.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomik23/autosuggest.git"
  },
  "scripts": {
    "dev": "concurrently \"yarn sass:global\" \"yarn sass:prod\" \"yarn sass:dev\" \"yarn watch\"",
    "prod": "yarn build && yarn sass:prod",
    "build": "rollup --config --environment PRODUCTION",
    "watch": "rollup --config --watch",
    "sass:dev": "node-sass ./sources/scss/style.scss ./docs/autosuggest.min.css -w",
    "sass:prod": "node-sass ./sources/scss/style.scss ./docs/autosuggest.min.css --output-style compressed",
    "sass:global": "node-sass ./sources/scss/global.scss ./docs/global.min.css --output-style compressed",
    "postcss": "postcss ./docs/style.min.css -r"
  },
  "keywords": [
    "autosuggest",
    "autocomplete",
    "WAI-ARIA",
    "search",
    "javascript",
    "promise+fetch",
    "axios"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-babel": "^5.1.0",
    "autoprefixer": "^9.8.6",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "cssnano": "^4.1.10",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "node-sass": "^4.14.1",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "promise-polyfill": "^8.1.3",
    "rollup": "^2.23.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^3.1.5",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-terser": "^7.0.0"
  }
}
