{
  "name": "autosuggest",
  "version": "1.0.22",
  "description": "Simple autocomplete component in vanilla JS",
  "author": "Grzegorz Tomicki",
  "main": "docs/js/autosuggest.min.js",
  "browser": "docs/js/autosuggest.umd.min.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomik23/autosuggest.git"
  },
  "scripts": {
    "dev": "concurrently \"yarn sass:dev\" \"yarn sass:prod\" \"yarn watch\"",
    "prod": "yarn build && yarn sass:prod",
    "build": "rollup -c --environment PRODUCTION",
    "watch": "rollup -c -w",
    "sass:prod": "node-sass sources/scss -o docs/css --output-style compressed",
    "sass:dev": "node-sass -w sources/scss -o docs/css"
  },
  "keywords": [
    "autosuggest",
    "autocomplete",
    "WAI-ARIA",
    "search",
    "javascript",
    "promise+fetch",
    "axios"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.1",
    "autoprefixer": "^10.0.2",
    "concurrently": "^5.3.0",
    "core-js": "^3.7.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "node-sass": "^5.0.0",
    "postcss-cli": "^8.3.0",
    "prettier": "^2.2.0",
    "rollup": "^2.33.3",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}