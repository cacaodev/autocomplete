{
  "name": "autosuggest",
  "version": "1.0.3",
  "main": "index.js",
  "license": "MIT",
  "author": "Grzegorz Tomicki <gtomik@gmail.com>",
  "scripts": {
    "dev": "concurrently \"yarn build:dev\" \"yarn build:country\" \"yarn build:persons\"",
    "build:dev": "webpack-dev-server --config webpack.config.js --mode development --open",
    "prod": "webpack --config webpack.config.js --mode production",
    "build:country": "json-server --watch static/country.json --port 3004",
    "build:persons": "json-server --watch static/persons.json --port 3005"
  },
  "keywords": [
    "autosuggest",
    "async/await",
    "fetch"
  ],
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.1.4",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "dotenv-webpack": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "browserslist": [
    "last 2 version",
    ">1%",
    "not dead"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "browser-sync": "^2.26.7",
    "concurrently": "^4.1.1",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "json-server": "^0.15.0",
    "prettier": "^1.18.2"
  }
}