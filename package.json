{
  "name": "autocomplete",
  "version": "1.0.29",
  "description": "Simple autocomplete component in vanilla JS",
  "author": "Grzegorz Tomicki",
  "main": "docs/js/autocomplete.min.js",
  "browser": "docs/js/autocomplete.umd.min.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomik23/autocomplete.git"
  },
  "scripts": {
    "dev": "concurrently \"yarn sass:dev\" \"yarn sass:prod\" \"yarn watch\"",
    "prod": "yarn build && yarn sass:prod",
    "build": "rollup -c --environment PRODUCTION",
    "watch": "rollup -c -w",
    "sass:prod": "node-sass sources/scss -o docs/css --output-style compressed",
    "sass:dev": "node-sass -w sources/scss -o docs/css"
  },
  "keywords": [
    "autocomplete",
    "WAI-ARIA",
    "search",
    "javascript",
    "promise+fetch",
    "axios"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^5.2.2",
    "autoprefixer": "^10.2.3",
    "concurrently": "^5.3.0",
    "core-js": "^3.8.3",
    "cssnano": "^4.1.10",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "node-sass": "^5.0.0",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.2.1",
    "rollup": "^2.37.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}