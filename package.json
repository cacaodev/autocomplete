{
  "name": "autocomplete",
  "version": "1.6.9",
  "description": "Simple autocomplete component in vanilla JS",
  "author": "Grzegorz Tomicki",
  "main": "dist/js/autocomplete.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomik23/autocomplete.git"
  },
  "scripts": {
    "dev": "concurrently \"yarn sass:dev\" \"yarn sass:prod\" \"yarn watch\"",
    "prod": "yarn build && yarn sass:prod && yarn postcss:global && yarn postcss:autocomplete",
    "build": "rollup -c --environment PRODUCTION",
    "watch": "rollup -c -w",
    "sass:prod": "sass --no-source-map sources/scss:docs/css",
    "sass:dev": "sass --watch sources/scss:docs/css",
    "postcss:global": "postcss docs/css --dir docs/css",
    "postcss:autocomplete": "postcss --ext min.css docs/css/autocomplete.css --dir dist/css/"
  },
  "keywords": [
    "autocomplete",
    "WAI-ARIA",
    "search",
    "javascript",
    "promise+fetch",
    "axios"
  ],
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.3.0",
    "core-js": "^3.19.1",
    "cssnano": "^5.0.10",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "postcss-css-variables": "^0.18.0",
    "prettier": "^2.4.1",
    "rollup": "^2.59.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.43.4"
  }
}
